    "babel-preset-next": "^1.1.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.2",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7",
    "url-loader": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "react": "^16.2.0",
